<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1300">
        <KeyFrameSet>
            <KeyPosition
                motion:framePosition="110"
                motion:motionTarget="@id/first_user_name"
                motion:percentY="1" />
            <KeyPosition
                motion:framePosition="110"
                motion:motionTarget="@id/second_user_name"
                motion:percentY="1" />
            <KeyPosition
                motion:framePosition="110"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@id/third_user_name" />
            <KeyPosition
                motion:framePosition="99"
                motion:motionTarget="@id/first_user_progress"
                motion:percentY="1" />
            <KeyPosition
                motion:framePosition="99"
                motion:motionTarget="@id/second_user_progress"
                motion:percentY="1" />
            <KeyPosition
                motion:framePosition="99"
                motion:motionTarget="@id/third_user_progress"
                motion:percentY="1" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/materialCardView4"
            android:layout_width="153dp"
            android:layout_height="0dp"
            android:visibility="invisible"
            motion:layout_constraintBottom_toTopOf="@+id/guideline4"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView3"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView3" />
        <Constraint
            android:id="@+id/materialCardView3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="20dp"
            android:visibility="invisible"
            motion:layout_constraintBottom_toBottomOf="@+id/materialCardView4"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/the_first_user_poster"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="-30dp"
            android:elevation="10dp"
            android:visibility="invisible"
            motion:layout_constraintBottom_toTopOf="@+id/materialCardView4"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView4"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView4" />
        <Constraint
            android:id="@+id/the_second_user_poster"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="-10dp"
            android:elevation="10dp"
            android:visibility="invisible"
            motion:layout_constraintEnd_toStartOf="@+id/materialCardView4"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView3"
            motion:layout_constraintTop_toTopOf="@+id/materialCardView4" />
        <Constraint
            android:id="@+id/the_third_user_poster"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginTop="-10dp"
            android:elevation="10dp"
            android:visibility="invisible"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView3"
            motion:layout_constraintStart_toEndOf="@+id/materialCardView4"
            motion:layout_constraintTop_toTopOf="@+id/materialCardView4" />
        <Constraint
            android:id="@+id/the_first_user_poster_outline"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:elevation="10dp"
            android:visibility="invisible"
            motion:layout_constraintBottom_toBottomOf="@+id/the_first_user_poster"
            motion:layout_constraintEnd_toEndOf="@+id/the_first_user_poster"
            motion:layout_constraintStart_toStartOf="@+id/the_first_user_poster"
            motion:layout_constraintTop_toTopOf="@+id/the_first_user_poster" />
        <Constraint
            android:id="@+id/the_third_user_poster_outline"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:elevation="10dp"
            android:visibility="invisible"
            motion:layout_constraintBottom_toBottomOf="@+id/the_third_user_poster"
            motion:layout_constraintEnd_toEndOf="@+id/the_third_user_poster"
            motion:layout_constraintStart_toStartOf="@+id/the_third_user_poster"
            motion:layout_constraintTop_toTopOf="@+id/the_third_user_poster" />
        <Constraint
            android:id="@+id/the_second_user_poster_outline"
            android:layout_width="10dp"
            android:layout_height="10dp"
            android:elevation="10dp"
            android:visibility="invisible"
            motion:layout_constraintBottom_toBottomOf="@+id/the_second_user_poster"
            motion:layout_constraintEnd_toEndOf="@+id/the_second_user_poster"
            motion:layout_constraintStart_toStartOf="@+id/the_second_user_poster"
            motion:layout_constraintTop_toTopOf="@+id/the_second_user_poster" />
        <Constraint
            android:id="@+id/first_user_progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:elevation="10dp"
            android:visibility="invisible"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView4"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView4"
            motion:layout_constraintTop_toBottomOf="@+id/first_user_name" />
        <Constraint
            android:id="@+id/third_user_progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:elevation="10dp"
            android:visibility="invisible"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView3"
            motion:layout_constraintStart_toEndOf="@+id/materialCardView4"
            motion:layout_constraintTop_toBottomOf="@+id/third_user_name" />
        <Constraint
            android:id="@+id/second_user_progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:elevation="10dp"
            android:visibility="invisible"
            motion:layout_constraintEnd_toStartOf="@+id/materialCardView4"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView3"
            motion:layout_constraintTop_toBottomOf="@+id/second_user_name" />
        <Constraint
            android:id="@+id/rank_the_second_image"
            android:elevation="10dp"
            motion:layout_constraintEnd_toEndOf="@+id/the_second_user_poster"
            android:layout_width="30dp"
            android:layout_height="30dp"
            motion:layout_constraintBottom_toBottomOf="@+id/the_second_user_poster"
            android:layout_marginBottom="-10dp"
            android:visibility="gone"
            motion:layout_constraintStart_toStartOf="@+id/the_second_user_poster" />
        <Constraint
            android:id="@+id/rank_the_third_image"
            android:elevation="10dp"
            motion:layout_constraintEnd_toEndOf="@+id/the_third_user_poster"
            android:layout_width="30dp"
            android:layout_height="30dp"
            motion:layout_constraintBottom_toBottomOf="@+id/the_third_user_poster"
            android:layout_marginBottom="-10dp"
            android:visibility="gone"
            motion:layout_constraintStart_toStartOf="@+id/the_third_user_poster" />
        <Constraint
            android:id="@+id/rank_the_first_image"
            android:elevation="10dp"
            motion:layout_constraintEnd_toEndOf="@+id/the_first_user_poster"
            android:layout_width="30dp"
            android:layout_height="30dp"
            motion:layout_constraintBottom_toBottomOf="@+id/the_first_user_poster"
            android:layout_marginBottom="-10dp"
            android:visibility="gone"
            motion:layout_constraintStart_toStartOf="@+id/the_first_user_poster" />
        <Constraint
            android:layout_height="0dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_width="0dp"
            motion:layout_constraintTop_toBottomOf="@+id/guideline4"
            android:id="@+id/rating_recycler_view" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="-2dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toTopOf="@+id/the_first_user_poster"
            motion:layout_constraintEnd_toEndOf="@+id/the_first_user_poster"
            motion:layout_constraintStart_toStartOf="@+id/the_first_user_poster" />
        <Constraint
            android:id="@+id/the_first_user_poster"
            android:layout_width="102dp"
            android:layout_height="102dp"
            android:layout_marginBottom="-30dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toTopOf="@+id/materialCardView4"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView4"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView4" />
        <Constraint
            android:id="@+id/the_first_user_poster_outline"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@+id/the_first_user_poster"
            motion:layout_constraintEnd_toEndOf="@+id/the_first_user_poster"
            motion:layout_constraintStart_toStartOf="@+id/the_first_user_poster"
            motion:layout_constraintTop_toTopOf="@+id/the_first_user_poster" />
        <Constraint
            android:id="@+id/the_second_user_poster"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:layout_marginTop="-10dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@+id/materialCardView4"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView3"
            motion:layout_constraintTop_toTopOf="@+id/materialCardView4" />
        <Constraint
            android:id="@+id/the_second_user_poster_outline"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@+id/the_second_user_poster"
            motion:layout_constraintEnd_toEndOf="@+id/the_second_user_poster"
            motion:layout_constraintStart_toStartOf="@+id/the_second_user_poster"
            motion:layout_constraintTop_toTopOf="@+id/the_second_user_poster" />
        <Constraint
            android:id="@+id/the_third_user_poster"
            android:layout_width="85dp"
            android:layout_height="85dp"
            android:layout_marginTop="-10dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView3"
            motion:layout_constraintStart_toEndOf="@+id/materialCardView4"
            motion:layout_constraintTop_toTopOf="@+id/materialCardView4" />
        <Constraint
            android:id="@+id/the_third_user_poster_outline"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@+id/the_third_user_poster"
            motion:layout_constraintEnd_toEndOf="@+id/the_third_user_poster"
            motion:layout_constraintStart_toStartOf="@+id/the_third_user_poster"
            motion:layout_constraintTop_toTopOf="@+id/the_third_user_poster" />
        <Constraint
            android:id="@+id/rank_the_first_image"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginBottom="-10dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@+id/the_first_user_poster"
            motion:layout_constraintEnd_toEndOf="@+id/the_first_user_poster"
            motion:layout_constraintStart_toStartOf="@+id/the_first_user_poster" />
        <Constraint
            android:id="@+id/rank_the_second_image"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginBottom="-10dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@+id/the_second_user_poster"
            motion:layout_constraintEnd_toEndOf="@+id/the_second_user_poster"
            motion:layout_constraintStart_toStartOf="@+id/the_second_user_poster" />
        <Constraint
            android:id="@+id/rank_the_third_image"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:layout_marginBottom="-10dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintBottom_toBottomOf="@+id/the_third_user_poster"
            motion:layout_constraintEnd_toEndOf="@+id/the_third_user_poster"
            motion:layout_constraintStart_toStartOf="@+id/the_third_user_poster" />
        <Constraint
            android:id="@+id/first_user_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView4"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView4"
            motion:layout_constraintTop_toBottomOf="@+id/the_first_user_poster" />
        <Constraint
            android:id="@+id/second_user_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@+id/materialCardView4"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView3"
            motion:layout_constraintTop_toBottomOf="@+id/rank_the_second_image" />
        <Constraint
            android:id="@+id/third_user_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView3"
            motion:layout_constraintStart_toEndOf="@+id/materialCardView4"
            motion:layout_constraintTop_toBottomOf="@+id/rank_the_third_image" />
        <Constraint
            android:id="@+id/first_user_progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView4"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView4"
            motion:layout_constraintTop_toBottomOf="@+id/first_user_name" />
        <Constraint
            android:id="@+id/second_user_progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toStartOf="@+id/materialCardView4"
            motion:layout_constraintStart_toStartOf="@+id/materialCardView3"
            motion:layout_constraintTop_toBottomOf="@+id/second_user_name" />
        <Constraint
            android:id="@+id/third_user_progress"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:elevation="10dp"
            android:visibility="visible"
            motion:layout_constraintEnd_toEndOf="@+id/materialCardView3"
            motion:layout_constraintStart_toEndOf="@+id/materialCardView4"
            motion:layout_constraintTop_toBottomOf="@+id/third_user_name" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/guideline4"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/rating_recycler_view"
            android:layout_marginTop="8dp" />
    </ConstraintSet>
</MotionScene>