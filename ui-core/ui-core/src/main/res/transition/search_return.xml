<?xml version="1.0" encoding="utf-8"?>
<!--suppress AndroidElementNotAllowed -->
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
               android:duration="300"
               android:transitionOrdering="together">

    <explode android:interpolator="@android:interpolator/accelerate_cubic" android:duration="600">
        <targets>
            <target android:targetId="@+id/search_view" />
        </targets>
    </explode>

    <fade android:duration="300" android:interpolator="@android:interpolator/decelerate_cubic"/>

    <targets>
        <target android:excludeId="@android:id/statusBarBackground"/>
        <target android:excludeId="@android:id/navigationBarBackground"/>
    </targets>
</transitionSet>