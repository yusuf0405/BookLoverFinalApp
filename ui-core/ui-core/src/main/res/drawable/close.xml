<?xml version="1.0" encoding="utf-8"?>
<rotate xmlns:android="http://schemas.android.com/apk/res/android"
    android:fromDegrees="45"
    android:toDegrees="45"
    android:visible="true">

    <layer-list>
        <item android:top="6dp" android:left="15dp" android:bottom="6dp" android:right="15dp">
            <shape android:shape="rectangle">
                <stroke android:width="1dp" android:color="?colorControlNormal"/>
                <size android:width="2dp" android:height="2dp" />
            </shape>
        </item>

        <item android:top="15dp" android:left="6dp" android:bottom="15dp" android:right="6dp">
            <shape android:shape="rectangle">
                <stroke android:width="1dp" android:color="?colorControlNormal"/>
                <size android:width="2dp" android:height="2dp" />
            </shape>
        </item>
    </layer-list>

</rotate>