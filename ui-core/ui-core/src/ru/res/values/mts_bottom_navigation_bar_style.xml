        <item name="mtsBottomNavigationColorBarTextChecked">@color/red_mts</item>
        <item name="mtsBottomNavigationColorBarIconUnchecked">#AEB5BD</item>
        <item name="mtsBottomNavigationColorBarTextUnchecked">#AEB5BD</item>
